{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": false
    },
    "description": {
      "type": "richtext"
    },
    "img": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "All",
        "alien",
        "human"
      ],
      "default": "All"
    },
    "on": {
      "type": "boolean"
    }
  }
}